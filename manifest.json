{
  "name": "Dark Mode Every Where",
  "description": "Makes every website dark with white text.",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["scripting", "tabs", "activeTab"],
  "action": {
    "default_title": "Activate Dark Mode",
    "default_popup" : "popup.html"
  },
  "host_permissions": ["<all_urls>"]
}
